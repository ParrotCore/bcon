{
  "name": "bcon-parser",
  "version": "2.3.0",
  "description": "Parser for BCON (BCON Configures Only Nicely) language.",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "type": "commonjs",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node ./test/tests.js && node ./test/test-type-validation.js && node ./test/test-nested-constructors.js && node ./test/test-spread-objects.js && node ./test/test-constructor-arguments.js",
    "test:unit": "node ./test/tests.js",
    "test:types": "node ./test/test-type-validation.js",
    "test:constructors": "node ./test/test-nested-constructors.js",
    "test:spread": "node ./test/test-spread-objects.js",
    "test:args": "node ./test/test-constructor-arguments.js",
    "performance": "node ./test/performance.js",
    "compare": "node ./test/comparison.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parrotcore/bcon-parser.git"
  },
  "keywords": [
    "bcon",
    "configuration",
    "config",
    "parser",
    "esm",
    "commonjs"
  ],
  "author": "parrotcore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parrotcore/bcon-parser/issues"
  },
  "homepage": "https://github.com/parrotcore/bcon-parser#readme"
}
